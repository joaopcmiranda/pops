{
  "name": "@pops/test-utils",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@pops/types": "workspace:*",
    "drizzle-orm": "^0.35.3",
    "fastify": "^5.0.0",
    "pg": "^8.11.3"
  },
  "peerDependencies": {
    "vitest": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "prettier": "^3.6.2",
    "tsup": "^8.0.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "import": "./dist/database.mjs",
      "require": "./dist/database.js"
    },
    "./fastify": {
      "types": "./dist/fastify.d.ts",
      "import": "./dist/fastify.mjs",
      "require": "./dist/fastify.js"
    },
    "./fixtures": {
      "types": "./dist/fixtures.d.ts",
      "import": "./dist/fixtures.mjs",
      "require": "./dist/fixtures.js"
    }
  }
}
