{
  "name": "@pops/tools",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ci": "yarn typecheck && yarn lint && yarn format && yarn test",
    "import:anz": "tsx src/import-anz.ts",
    "import:amex": "tsx src/import-amex.ts",
    "import:ing": "tsx src/import-ing.ts",
    "import:up": "tsx src/import-up-batch.ts",
    "match:transfers": "tsx src/match-transfers.ts",
    "match:novated": "tsx src/match-novated.ts",
    "entities:create": "tsx src/create-entities.ts",
    "entities:lookup": "tsx src/regenerate-entity-lookup.ts",
    "audit": "tsx src/audit-notion.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@notionhq/client": "^2.2.15",
    "better-sqlite3": "^11.7.0",
    "csv-parse": "^5.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.12.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
